/* latex.acc */

%prelude{
   #include <stdio.h>
   #include <malloc.h>

   void fin_analyse() {
      printf("syntaxe ok");
   }
}

/* Declaration des tokens */
%token BEGINS, TITLE, AUTHOR, DATE, MAKETITLE, SECTION, SUBSECTION,
SUBSUBSECTION, TEXTBF, TEXTIT, ITEMIZE, ENUMERATE, ITEM, BACKSLASH,
LDOTS, WORD, END, ACC;

/* Grammaire */
S : TITLE | WORD { fin_analyse(); };